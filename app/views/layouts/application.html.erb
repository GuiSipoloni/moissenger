<!DOCTYPE html>
<html>
  <head>
    <title>Twitter Moip</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <!-- Wrapper -->
    <div id="wrapper">

      <!-- Header -->
      <%= render partial: "header" %>

      <!-- Main -->
      <div id="main">
        <%= yield %>
      </div>
    </div>

  </body>
</html>
